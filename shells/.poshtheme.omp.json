{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "horizontal_offset": 0,
      "segments": [
        {
          "type": "path",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "background": "#feae34",
          "foreground": "#262b44",
          "properties": {
            "prefix": "  ",
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "",
          "background": "#fee761",
          "foreground": "#262b44",
          "properties": {
            "ahead_and_behind_color": "#e43b44",
            "ahead_color": "#2ce8f5",
            "behind_color": "#f77622",
            "display_stash_count": true,
            "display_upstream_icon": true,
            "local_changes_color": "#f77622",
            "stash_count_icon": " ",
            "status_colors_enabled": true
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "",
          "background": "#fee761",
          "foreground": "#262b44"
        },
        {
          "type": "exit",
          "style": "diamond",
          "leading_diamond": "<transparent, #0095e9></>",
          "trailing_diamond": "",
          "background": "#0095e9",
          "foreground": "#ffffff",
          "properties": {
            "always_enabled": true,
            "color_background": true,
            "display_exit_code": false,
            "error_color": "#ff0044",
            "prefix": " "
          }
        }
      ],
      "type": "prompt",
      "vertical_offset": 0
    }
  ],
  "final_space": true
}
