[
  {
    "defaults": {
      "link": {
        "create": true,
        "relink": true,
        "force": true
      }
    }
  },
  {
    "shell": [
      ["sudo gem install tmuxinator", "Install tmuxinator"]
    ]
  },
  {
    "link": {
      "~/bin/battery.sh": "apps/tmux/bin/battery.sh",
      "~/bin/tmux-mem-cpu-load": "apps/tmux/bin/tmux-mem-cpu-load",
      "~/.tmuxinator": "apps/tmux/tmuxinator",
      "~/.tmux.conf": "apps/tmux/tmux.conf",
      "~/.tmux.live.conf": "apps/tmux/tmux.live.conf",
      "~/.tmux/solarized": "apps/tmux/tmux/solarized"
    }
  }
]
